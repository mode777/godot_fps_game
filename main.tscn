	Events.debug.connect(on_debug)
	
func on_debug(key: String, value):
	debug_info[key] = value
	text = format()
	
func format():
	var str = \"\"
	for key in debug_info.keys():
		str += \"%s: %s\\n\" % [key,debug_info[key]]
	return str
